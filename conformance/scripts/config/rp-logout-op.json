{
  "alias": "authrim-rp-logout-op",
  "description": "Automated Certification Test for Authrim RP-Initiated Logout OP",
  "server": {
    "discoveryUrl": "{ISSUER}/.well-known/openid-configuration"
  },
  "client": {
    "scope": "openid profile email",
    "redirect_uri": "https://www.certification.openid.net/test/a/{alias}/callback",
    "post_logout_redirect_uri": "https://www.certification.openid.net/test/a/{alias}/post_logout_redirect"
  },
  "client2": {
    "scope": "openid profile email",
    "redirect_uri": "https://www.certification.openid.net/test/a/{alias}/callback",
    "post_logout_redirect_uri": "https://www.certification.openid.net/test/a/{alias}/post_logout_redirect"
  },
  "browser": {
    "show_console_log": true
  }
}
