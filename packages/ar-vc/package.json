{
  "name": "@authrim/ar-vc",
  "version": "0.1.6",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "wrangler dev --config wrangler.dev.toml 2>/dev/null || wrangler dev",
    "deploy": "[ -n \"$DEPLOY_ENV\" ] && wrangler deploy --config wrangler.$DEPLOY_ENV.toml || wrangler deploy",
    "build": "tsc",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src --ext .ts",
    "test": "npx vitest run",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@authrim/ar-lib-core": "workspace:*",
    "hono": "^4.11.4",
    "jose": "^6.1.2",
    "tslib": "^2.8.1"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20251121.0",
    "@eslint/js": "^9.22.0",
    "@typescript-eslint/eslint-plugin": "^8.29.1",
    "@typescript-eslint/parser": "^8.29.1",
    "eslint": "^9.22.0",
    "globals": "^16.0.0",
    "typescript": "^5.9.3",
    "vitest": "^2.1.8",
    "wrangler": "^4.53.0"
  }
}
