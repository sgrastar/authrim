name = "enrai-op-auth"
main = "src/index.ts"
compatibility_date = "2024-09-23"
compatibility_flags = ["nodejs_compat"]
workers_dev = false
preview_urls = false

# KV Namespaces
[[kv_namespaces]]
binding = "AUTH_CODES"
id = "9344b611e11944ffb8e0caf2c7ab0110"
preview_id = "d655d33a22fa4c8e9642eb976acffe4d"

# D1 Database
[[d1_databases]]
binding = "DB"
database_name = "enrai-users-db"
database_id = "c286a9b6-b646-43a6-802d-15d527bc9efa"

[[kv_namespaces]]
binding = "STATE_STORE"
id = "4e55929a20184353afbcf7ba174445f4"
preview_id = "ad01aeef7042479c9790690367a703d0"

[[kv_namespaces]]
binding = "NONCE_STORE"
id = "9e8b666a67fc426d9d10a8aeaf0ace48"
preview_id = "4296c22b6fde4463ad1d3eb66ef29683"

[[kv_namespaces]]
binding = "CLIENTS"
id = "3284d7d7f6cb4640a42222a68e1e3f04"
preview_id = "965cfd6b286045c0a60c3ae8b0da8915"

[[kv_namespaces]]
binding = "RATE_LIMIT"
id = "39df7d0d6c024b1da2d5b8f37b8f15cd"
preview_id = "358414c119844e33b731bde80e0252bd"

# Durable Objects Bindings
[[durable_objects.bindings]]
name = "KEY_MANAGER"
class_name = "KeyManager"
script_name = "enrai-shared"

[[durable_objects.bindings]]
name = "SESSION_STORE"
class_name = "SessionStore"
script_name = "enrai-shared"

[[durable_objects.bindings]]
name = "AUTH_CODE_STORE"
class_name = "AuthorizationCodeStore"
script_name = "enrai-shared"

# Environment variables
[vars]
ISSUER_URL = "https://enrai.sgrastar.workers.dev"
TOKEN_EXPIRY = "3600"
CODE_EXPIRY = "120"
STATE_EXPIRY = "300"
NONCE_EXPIRY = "300"
REFRESH_TOKEN_EXPIRY = "2592000"
KEY_ID = "dev-key-1763100614021-fu4soi"
ALLOW_HTTP_REDIRECT = "true"
OPEN_REGISTRATION = "false"

# Development configuration
[dev]
port = 8788
