name = "authrim-op-management"
main = "src/index.ts"
compatibility_date = "2024-09-23"
compatibility_flags = ["nodejs_compat"]
workers_dev = false
preview_urls = false

# KV Namespaces
[[kv_namespaces]]
binding = "CLIENTS"
id = "3284d7d7f6cb4640a42222a68e1e3f04"
preview_id = "965cfd6b286045c0a60c3ae8b0da8915"

# D1 Database
[[d1_databases]]
binding = "DB"
database_name = "authrim-users-db"
database_id = "c286a9b6-b646-43a6-802d-15d527bc9efa"

[[kv_namespaces]]
binding = "REFRESH_TOKENS"
id = "3d83eddd260a40a18787db726b129dc9"
preview_id = "d3930a3f3b974a5682ac0d4abcbbb7a8"

[[kv_namespaces]]
binding = "REVOKED_TOKENS"
id = "49316f61064e451bb813d17d8d020c81"
preview_id = "79979c391e4947a6a0d15bddacdb7ba5"

[[kv_namespaces]]
binding = "RATE_LIMIT"
id = "39df7d0d6c024b1da2d5b8f37b8f15cd"
preview_id = "358414c119844e33b731bde80e0252bd"

[[kv_namespaces]]
binding = "INITIAL_ACCESS_TOKENS"
id = "78bf827d6aa046268cc46a34addb4fb5"
preview_id = "146e41ed3d104e60bee3ab8cf7b8f38f"

# Durable Objects Bindings
[[durable_objects.bindings]]
name = "KEY_MANAGER"
class_name = "KeyManager"
script_name = "authrim-shared"

[[durable_objects.bindings]]
name = "REFRESH_TOKEN_ROTATOR"
class_name = "RefreshTokenRotator"
script_name = "authrim-shared"

# Environment variables
[vars]
ISSUER_URL = "https://authrim.sgrastar.workers.dev"
TOKEN_EXPIRY = "3600"
CODE_EXPIRY = "120"
STATE_EXPIRY = "300"
NONCE_EXPIRY = "300"
REFRESH_TOKEN_EXPIRY = "2592000"
KEY_ID = "dev-key-1763100614021-fu4soi"
ALLOW_HTTP_REDIRECT = "true"
OPEN_REGISTRATION = "false"

# Development configuration
[dev]
port = 8791
