{
  "name": "@authrim/setup",
  "version": "0.1.102",
  "description": "CLI tool for setting up Authrim OIDC Provider on Cloudflare Workers",
  "type": "module",
  "bin": {
    "authrim-setup": "./dist/index.js"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "templates",
    "README.md"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsx src/index.ts",
    "start": "node dist/index.js",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src/",
    "test": "vitest run",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "commander": "^12.1.0",
    "@inquirer/prompts": "^7.2.1",
    "chalk": "^5.3.0",
    "ora": "^8.1.1",
    "zod": "^3.24.1",
    "degit": "^2.8.4",
    "execa": "^9.5.2",
    "@hono/node-server": "^1.19.9",
    "hono": "^4.11.4",
    "tar": "^7.4.3"
  },
  "devDependencies": {
    "@types/node": "^22.10.4",
    "@types/tar": "^6.1.13",
    "typescript": "^5.9.3",
    "vitest": "^2.1.8",
    "tsx": "^4.20.6"
  },
  "keywords": [
    "authrim",
    "openid-connect",
    "oidc",
    "cloudflare-workers",
    "cli",
    "setup"
  ],
  "author": "",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/sgrastar/authrim.git",
    "directory": "packages/setup"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
